<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:ovp="sap.ovp.ui"
	xmlns:template="http://schemas.sap.com/sapui5/extension/sap.ui.core.template/1" width="auto">
	<IconTabBar
		expanded="true"
		id="idIconTabBar"
		select="handleIconTabBarSelect"
		class="sapUiResponsiveContentPadding">
		<items>
			<IconTabFilter
				id ="Attributes"
				
				iconColor="Positive"
				design="Horizontal"
				count="{/ZMS_ATTRIBUTE_COUNT/attr_count} "
				text="Attributes"
				key="Ok" >
			</IconTabFilter>
			<IconTabSeparator icon="sap-icon://open-command-field" />
			<IconTabFilter
				id="MasterDataTypes"
			
				iconColor="Positive"
				design="Horizontal"
				count="{/ProductCollectionStats/Counts/Weight/Heavy} of {/ProductCollectionStats/Counts/Total}"
				text="Master Data Types"
				key="Heavy" >
			</IconTabFilter>
			<IconTabSeparator icon="sap-icon://open-command-field" />
			<IconTabFilter
				id="TimeProfiles"
			
				iconColor="Positive"
				design="Horizontal"
				count="{/ProductCollectionStats/Counts/Weight/Overweight} of {/ProductCollectionStats/Counts/Total}"
				text="Time Profiles"
				key="Overweight" >
			</IconTabFilter>
			<IconTabSeparator icon="sap-icon://open-command-field" />
			<IconTabFilter
				id="PlanningAreas"
				
				iconColor="Positive"
				design="Horizontal"
				count="{/ProductCollectionStats/Counts/Weight/Overweight} of {/ProductCollectionStats/Counts/Total}"
				text="Planning Areas"
				key="Overweight">
			
			</IconTabFilter>
		</items>
	</IconTabBar>
</core:FragmentDefinition>