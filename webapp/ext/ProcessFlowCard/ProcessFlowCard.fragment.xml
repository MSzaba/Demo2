<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:ovp="sap.ovp.ui"
	xmlns:template="http://schemas.sap.com/sapui5/extension/sap.ui.core.template/1" width="auto">
	<IconTabBar
		
		expanded="false"
		id="idIconTabBar"
		select="handleIconTabBarSelect"
		class="sapUiResponsiveContentPadding">
		<items>
			<IconTabFilter
				id ="Attributes"
				
			
				design="Vertical"
				count="{view>/AttributesCount}"
				text="Attributes"
				key="Ok" >
			</IconTabFilter>
			<IconTabSeparator icon="sap-icon://open-command-field" />
			<IconTabFilter
				id="MasterDataTypes"
			
		
				design="Horizontal"
				count="{view>/MasterDataTypesCount}"
				text="Master Data Types"
				key="Heavy" >
			</IconTabFilter>
			<IconTabSeparator icon="sap-icon://open-command-field" />
			<IconTabFilter
				id="TimeProfiles"
			
			
				design="Horizontal"
				count="{view>/TimeProfileCount}"
				text="Time Profiles"
				key="Overweight" >
			</IconTabFilter>
			<IconTabSeparator icon="sap-icon://open-command-field" />
			<IconTabFilter
				id="PlanningAreas"
				
		
				design="Horizontal"
				count="{view>/PlanningAreaCount}"
				text="Planning Areas"
				key="Overweight">
			
			</IconTabFilter>
		</items>
	</IconTabBar>
</core:FragmentDefinition>